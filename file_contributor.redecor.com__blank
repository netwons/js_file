<html><head><title></title><script type="text/javascript">function _blankLoad() {
      var h = document.location.href;
      // element_id used by htmlarea.js
      var i = h.indexOf('sysparm_element_id=');
      if (i > -1) {
        var id = h.substring(i + 19);
		if (window.parent.CustomEvent)
		  window.parent.CustomEvent.fire('blank.onload', id);
	  }
    }</script></head><body onload="_blankLoad();"></body></html>